{
    "name": "sass",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "watch:sass": "node-sass main.scss ../css/style.css -w",
        "compile:sass": "node-sass main.scss ../css/style.comp.css ",
        "concat:css": "concat -o ../css/style.concat.css ../css/style.comp.css",
        "compress:css": "node-sass ../css/style.comp.css ../css/style.css --output-style compressed",
        "build:css": "npm-run-all compile:sass concat:css compress:css"
    },
    "author": "Maria Sobota",
    "license": "ISC",
    "devDependencies": {
        "autoprefixer": "^10.4.19",
        "concat": "^1.0.3",
        "node-sass": "^9.0.0",
        "npm-run-all": "^4.1.5",
        "postcss": "^8.4.38",
        "postcss-cli": "^11.0.0"
    }
}